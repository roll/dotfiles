{
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "typescript", "typescriptreact"],
  "coc.preferences.willSaveHandlerTimeout": 5000,
  "tsserver.enableJavascript": false,
  "snippets.ultisnips.directories": ["~/.config/nvim/snipped"],
  "eslint.run": "onSave",
  "pyright.enable": true,
  "python.formatting.provider": "black",
  "pyright.organizeimports.provider": "isort",
  "python.linting.ruffEnabled": true,
  "pyright.inlayHints.variableTypes": false,
  "pyright.inlayHints.functionReturnTypes": false
}
